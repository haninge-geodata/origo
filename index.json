      "title": "Open street map byggnader",
      "type": "THREEDTILE",
      "abstract": "Byggnader från OSM",
      "url": "OSM-Buildings",
      "group": "root",
      "queryable": true,
      "maximumScreenSpaceError": 16,
      "showOutline": true,
      "outlineColor": "SNOW",
